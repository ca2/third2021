<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="aqua" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="basis">
				<Option output="$(#time_dir)/x64/$(TARGET_NAME)/lib$(PROJECT_NAME).so" prefix_auto="0" extension_auto="0" />
				<Option working_dir="$(#time_dir)/x64/$(TARGET_NAME)/" />
				<Option object_output="$(#time_dir)/intermediate/$(TARGET_NAME)/$(PROJECT_NAME)" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-fPIC" />
					<Add option="-m64" />
					<Add option="-g" />
					<Add option="-std=c++17" />
					<Add option="-D_DEBUG" />
					<Add directory="$(#archive.include)/platform-linux/third/include" />
				</Compiler>
				<Linker>
					<Add option="-m64" />
					<Add option="-Wl,-rpath=\\$$$ORIGIN" />
					<Add option="-ldl" />
					<Add option="-lrt" />
					<Add option="-lbfd" />
					<Add option="-lace" />
					<Add option="-Wl,-z,defs" />
				</Linker>
			</Target>
			<Target title="memdleak">
				<Option output="$(#time_dir)/x64/$(TARGET_NAME)/lib$(PROJECT_NAME).so" prefix_auto="0" extension_auto="0" />
