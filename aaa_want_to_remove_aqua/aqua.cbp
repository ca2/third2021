				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-g" />
					<Add option="-D_DEBUG" />
					<Add option="-DMEMDLEAK" />
				</Compiler>
			</Target>
			<Target title="stage">
				<Option output="$(#time_dir)/x64/$(TARGET_NAME)/lib$(PROJECT_NAME).so" prefix_auto="0" extension_auto="0" />
				<Option working_dir="$(#time_dir)/x64/$(TARGET_NAME)/" />
				<Option object_output="$(#time_dir)/intermediate/$(TARGET_NAME)/$(PROJECT_NAME)" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O0" />
					<Add option="-std=c++17" />
					<Add option="-pg" />
					<Add option="-fPIC" />
					<Add option="-m64" />
					<Add option="-g" />
					<Add directory="$(#archive.include)/platform-linux/third/include" />
				</Compiler>
				<Linker>
					<Add option="-pg" />
					<Add option="-m64" />
					<Add option="-Wl,-rpath=\\$$$ORIGIN" />
					<Add option="-ldl" />
					<Add option="-lrt" />
					<Add option="-lbfd" />
					<Add option="-lace" />
					<Add option="-Wl,-z,defs" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="all_sanitizers" targets="basis;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-fPIC" />
			<Add option="-std=c++17" />
			<Add option="-fexceptions" />
			<Add option="-fnon-call-exceptions" />
			<Add option="-fno-pie" />
			<Add option="-fno-stack-protector" />
			<Add option="-Wno-pointer-arith" />
			<Add option="-Wno-attributes" />
			<Add option="-Winvalid-pch" />
			<Add option="-Wno-conversion-null" />
